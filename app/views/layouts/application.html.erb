                            <a href="/users/auth/google_oauth2" class="services2"><%= image_tag "google_24.png", :alt => "Google" %></a>
                            <a href="/users/auth/twitter" class="services2"><%= image_tag "twitter_24.png", :alt => "Twitter" %></a>
